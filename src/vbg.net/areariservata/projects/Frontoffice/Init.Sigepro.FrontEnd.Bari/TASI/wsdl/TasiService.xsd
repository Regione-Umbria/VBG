<xs:simpleType>
<xs:restriction base="xs:string">
<xs:enumeration value="[a-zA-Z]{6}[0-9]{2}[a-zA-Z]{1}[0-9]{2}[a-zA-Z]{1}[0-9]{3}[a-zA-Z]{1}"/>
<xs:enumeration value="[0-9]{11}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="indirizzoSedeLegale" type="tns:indirizzoType">
