</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="70"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="dataNascita" type="tns:dataType"/>
<xs:element name="datiSocieta" type="tns:datiSocietaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati identificativi della società/ditta individuale nel caso di persona giuridica</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="indirizzoResidenza" type="tns:indirizzoType">
