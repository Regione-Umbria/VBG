</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="100"/>
<xs:pattern value="(([a-zA-Z0-9]+[\-._+])*[a-zA-Z0-9]+@([\-0-9a-zA-Z]+[.])+[a-zA-Z]{2,6})?"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:sequence>
</xs:complexType>

