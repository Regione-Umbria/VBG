</xs:simpleType>

<xs:complexType name="datiSocietaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di una societa/ditta individuale</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="caricaGiuridica" type="tns:caricaGiuridicaType">
<xs:annotation>
