  </system.net>
  <system.web>
    <httpRuntime maxRequestLength="100000" executionTimeout="60000" requestValidationMode="2.0"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID">
      <controls>

