    <Init.SIGePro.Manager.Properties.Settings>
      <setting name="SigeproSecurityUsername" serializeAs="String">
        <value>BACKOFFICE</value>
      </setting>
      <setting name="SigeproSecurityPassword" serializeAs="String">
        <value>9a44bd17bbd836547850c08a75d17011</value>
      </setting>
    </Init.SIGePro.Manager.Properties.Settings>

    <Init.SIGePro.DatiDinamici.Properties.Settings>
      <setting name="UsaJavascriptEmbedded" serializeAs="String">
        <value>True</value>
      </setting>
    </Init.SIGePro.DatiDinamici.Properties.Settings>

  </applicationSettings>
  <appSettings>
    <add key="WSHOSTURL_JAVA" value="http://172.29.2.41:8081/console"/>
    <!-- E' la cartella temporanea che verrÃ  utilizzata dalle varie applicazioni. Utilizzare Server.MapPath( ConfigurationSettings.AppSettings["TempPath"].ToString()) per individuare questo percorso fisico.-->
