    </diagnostics>
    <bindings configSource="bindings.config"/>
    <client>
      <endpoint address="http://172.29.2.41:8080/ibcsecurity/services" binding="basicHttpBinding" bindingConfiguration="sigeproSecurityHttpBinding" contract="WsSigeproSecurity.sigeproSecurity" name="sigeproSecurityService"/>
