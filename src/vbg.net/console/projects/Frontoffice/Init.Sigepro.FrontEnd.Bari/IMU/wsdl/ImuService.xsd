<?xml version='1.0' encoding='UTF-8'?><!-- Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is JAX-WS RI 2.2.8 svn-revision#13980. --><xs:schema xmlns:tns="http://www.comune.bari.it/serviziTributi" xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0" targetNamespace="http://www.comune.bari.it/serviziTributi">

<xs:element name="getDatiContribuenteRequest" type="tns:getDatiContribuenteRequestType"/>

<xs:element name="getDatiContribuenteResponse" type="tns:getDatiContribuenteResponseType"/>

<xs:element name="setRichiestaAgevolazioneRequest" type="tns:setRichiestaAgevolazioneRequestType"/>

<xs:element name="setRichiestaAgevolazioneResponse" type="tns:setRichiestaAgevolazioneResponseType"/>

<xs:complexType name="getDatiContribuenteRequestType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di una richiesta del servizio web IMU getDatiContribuente</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="datiAutorizzazione" type="tns:datiAutorizzazioneImuType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati necessari alla verifica dell'autorizzazione del sistema chiamante all'accesso al servizio</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiIdentificativi" type="tns:datiIdentificativiImuType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati identificativi del contribuente e del Caf facente da tramite</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiAutorizzazioneImuType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di autorizzazzione all'accesso di un servizio web IMU</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="identificativoUtente">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo dell'utente (sistema) richiedente il servizio</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{40}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="password">
<xs:annotation>
<xs:documentation xml:lang="it">Password dell'utente (sistema) richiedente il servizio</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{64}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="identificativoServizio">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo del servizio richiesto: I01 - getDatiContribuente, I02 - setRichiestaAgevolazione</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:enumeration value="I01"/>
<xs:enumeration value="I02"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="dataRichiesta" type="tns:dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Data di richiesta del servizio, nel formato GG/MM/AAAA</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="oraRichiesta">
<xs:annotation>
<xs:documentation xml:lang="it">Ora di richiesta del servizio, nel formato HH:MM:SS</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="([0-1][0-9]|[2][0-3]):([0-5][0-9]):([0-5][0-9]).[0-9]{3}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="idRichiesta">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo della richiesta</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{64}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="dataType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una data nel formato gg/mm/aaaa</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="(0[1-9]|[12][0-9]|3[01])/(0[1-9]|1[012])/(19[0-9]{2}|20[0-9]{2})"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiIdentificativiImuType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati identificativi di un contribuente per il servizio getDatiImmobili</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:datiCafImuType">
<xs:choice>
<xs:element name="codiceFiscaleContribuente" type="tns:codiceFiscaleType">
<xs:annotation>
<xs:documentation xml:lang="it">Codice fiscale Contribuente oppure partita IVA Contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="partitaIVAContribuente" type="tns:partitaIVAType">
<xs:annotation>
<xs:documentation xml:lang="it">Codice fiscale Contribuente oppure partita IVA Contribuente</xs:documentation>
</xs:annotation>
</xs:element>
</xs:choice>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="datiCafImuType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati utili al tracciamento di una richiesta di un servizio</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:choice>
<xs:element name="codiceFiscale" type="tns:codiceFiscaleType"/>
<xs:element name="partitaIVA" type="tns:partitaIVAType"/>
</xs:choice>
<xs:element name="indirizzoPec">
<xs:annotation>
<xs:documentation xml:lang="it">Indirizzo di Posta Elettronica Certificata del CAF</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="([a-zA-Z0-9]+[\-._+])*[a-zA-Z0-9]+@([\-0-9a-zA-Z]+[.])+[a-zA-Z]{2,6}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="codiceFiscaleType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il codice fiscale di una persona fisica</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[a-zA-Z]{6}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){2}[a-zA-Z]{1}([0-9]|[l-vL-V]){3}[a-zA-Z]{1}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="partitaIVAType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la partita IVA</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{11}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="getDatiContribuenteResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di risposta del servizio web IMU getDatiContribuente</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="return" type="tns:datiContribuenteResponseType"/>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiContribuenteResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati specifici di una risposta del servizio web IMU getDatiContribuente</xs:documentation>
</xs:annotation>
<xs:complexContent>
<xs:extension base="tns:commonResponseType">
<xs:sequence>
<xs:element name="datiContribuente" type="tns:datiContribuenteImuResponseType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Dati anagrafici del contribuente e degli immobili posseduti</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:extension>
</xs:complexContent>
</xs:complexType>

<xs:complexType name="commonResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati comuni di una risposta di un servizio web Tares</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="idRichiesta">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo della richiesta</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:pattern value="[a-fA-F0-9]{64}"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="code" type="xs:unsignedShort">
<xs:annotation>
<xs:documentation xml:lang="it">Codice di ritorno della risposta del servizio: 100 = Utente e/o password errati, 101 = Utente non abilitato all'uso del servizio, 110 = Richiesta non valida, 111 = Si e' verificato un errore durante la validazione della richiesta, 112 = Richiesta di agevolazione gia' esistente, 120 = L'elaborazione della richiesta non ha prodotto risultati, 999 = Si e' verificato un errore imprevisto nel collegamento con il backend</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="messaggio" type="xs:string" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Messaggio di errore. Presente solo se code diverso da 0</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:complexType name="datiContribuenteImuResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un contribuente IMU</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="idContribuente" type="tns:identificativoContribuenteType">
<xs:annotation>
<xs:documentation xml:lang="it">Identificativo univoco contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice>
<xs:element name="personaFisica" type="tns:personaFisicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati anagrafici del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="societa" type="tns:societaResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati anagrafici del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
</xs:choice>
<xs:element name="indirizzoPec" type="tns:emailType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Indirizzo di Posta Elettronica Certificata del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiResidenza" type="tns:datiIndirizzoResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati della residenza del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="immobili">
<xs:complexType>
<xs:sequence>
<xs:element name="datiImmobile" type="tns:datiImmobileResponseType" minOccurs="1" maxOccurs="unbounded">
<xs:annotation>
<xs:documentation xml:lang="it">Dati di un immobile o pertinenza posseduti dal contribuente</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="identificativoContribuenteType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per l'identificativo univoco di un contribuente</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:int">
<xs:maxInclusive value="999999"/>
<xs:minInclusive value="1"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="personaFisicaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati anagrafici di un contribuente IMU di tipo persona fisica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="cognome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="150"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="nome">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="156"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="codiceFiscale" type="tns:codiceFiscaleType"/>
<xs:element name="sesso" type="tns:sessoType" minOccurs="0"/>
<xs:element name="comuneNascita" type="tns:comuneNascitaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Comune italiano o stato estero di nascita del contribuente</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="provinciaNascita" type="tns:siglaProvinciaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Provincia di nascita del contribuente. Sarà presente solo se nato in Italia</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="comuneEsteroNascita" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione del comune estero di nascita del contribuente. Sarà presente solo se nato all'estero</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="70"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="dataNascita" type="tns:dataType" minOccurs="0"/>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="comuneNascitaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il comune di nascita di una persona fisica codificato tramite il codice ISTAT dei comuni italiani o tramite codice ISTAT degli stati esteri (se nato all'estero)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{6}|[0-9]{3}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="siglaProvinciaType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la sigla di una provincia italiana</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[A-Z]{2}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="societaResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati anagrafici di un contribuente IMU di tipo persona giuridica</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="denominazione">
<xs:annotation>
<xs:documentation xml:lang="it">Denominazione della società</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="306"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="partitaIva" type="tns:partitaIVAType"/>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="emailType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un indirizzo email</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="254"/>
<xs:pattern value="([a-zA-Z0-9]+[\-._+])*[a-zA-Z0-9]+@([\-0-9a-zA-Z]+[.])+[a-zA-Z]{2,6}"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiIndirizzoResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per un indirizzo di residenza di un un contribuente del Comune di Bari</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="descResidenza" type="xs:string">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione del comune italiano o dello Stato estero di residenza</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="provincia" type="tns:siglaProvinciaType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Sigla della provincia di residenza</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="cap" type="tns:capType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Codice Avviamento Postale</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice>
<xs:element name="viaCodificata" type="tns:viaCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione della via. Specificare viaCodificata se trattasi di via codificata secondo il SIT comunale, altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="viaNonCodificata" type="tns:viaNonCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Descrizione della via. Specificare viaCodificata se trattasi di via codificata secondo il SIT comunale, altrimenti viaNonCodificata</xs:documentation>
</xs:annotation>
</xs:element>
</xs:choice>
<xs:element name="numeroCivico" type="tns:civicoType" minOccurs="0"/>
<xs:element name="esponente" type="tns:esponenteCivicoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Esponente del numero civico (es. /H)</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="palazzina" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="7"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="scala" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="3"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="piano" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="3"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="interno" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="3"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="suffisso" type="tns:suffissoType" minOccurs="0"/>
<xs:element name="frazione" type="tns:frazioneType" minOccurs="0"/>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="capType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il Codice di Avviamento Postale (C.A.P.)</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{5}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="viaCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una via del comune di Bari codificata secondo il SIT comunale</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:pattern value="[0-9]{1,9}"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="viaNonCodificataType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per una via tramite una descrizione testuale</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="200"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="civicoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un numero civico</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:int">
<xs:maxInclusive value="99999"/>
<xs:minInclusive value="1"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="esponenteCivicoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per un esponente di un numero civico</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="20"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="suffissoType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il suffisso in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="20"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="frazioneType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per la frazione in un indirizzo</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:string">
<xs:maxLength value="20"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiImmobileResponseType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per i dati di un immobile</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="idImmobile" type="tns:identificativoImmobileType">
<xs:annotation>
<xs:documentation xml:lang="it">Codice identificativo dell'immobile</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="ubicazione" type="tns:datiIndirizzoImmobileResponse">
<xs:annotation>
<xs:documentation xml:lang="it">Ubicazione dell'immobile/fabbricato/area</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="datiCatastali" type="tns:datiCatastaliType">
<xs:annotation>
<xs:documentation xml:lang="it">Dati catastali dell'immobile/fabbricato/area</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="categoriaCatastale" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Categoria catastale dell'immobile/fabbricato/area</xs:documentation>
</xs:annotation>
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="4"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
<xs:element name="percentualePossesso" type="tns:percentualePossessoType">
<xs:annotation>
<xs:documentation xml:lang="it">Percentuale di possesso dell'immobile/fabbricato/area</xs:documentation>
</xs:annotation>
</xs:element>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="identificativoImmobileType">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato semplice per il codice identificativo di un immobile</xs:documentation>
</xs:annotation>
<xs:restriction base="xs:integer">
<xs:maxInclusive value="9999999999"/>
<xs:minInclusive value="1"/>
</xs:restriction>
</xs:simpleType>

<xs:complexType name="datiIndirizzoImmobileResponse">
<xs:annotation>
<xs:documentation xml:lang="it">Tipo di dato complesso per un indirizzo di un immobile</xs:documentation>
</xs:annotation>
<xs:sequence>
<xs:element name="cap" type="tns:capType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Codice Avviamento Postale</xs:documentation>
</xs:annotation>
</xs:element>
<xs:choice>
<xs:element name="viaCodificata" type="tns:viaCodificataType"/>
<xs:element name="viaNonCodificata" type="tns:viaNonCodificataType"/>
</xs:choice>
<xs:element name="civico" type="tns:civicoType" minOccurs="0"/>
<xs:element name="esponente" type="tns:esponenteCivicoType" minOccurs="0">
<xs:annotation>
<xs:documentation xml:lang="it">Esponente del numero civico</xs:documentation>
</xs:annotation>
</xs:element>
<xs:element name="palazzina" minOccurs="0">
<xs:simpleType>
<xs:restriction base="xs:string">
<xs:maxLength value="7"/>
</xs:restriction>
</xs:simpleType>
</xs:element>
